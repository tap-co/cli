{
  "name": "@tap-co/cli",
  "version": "0.2.0",
  "description": "Tap CLI - Plan campaigns, search inventory, and generate creatives from your terminal",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "bin": {
    "tap": "dist/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "start": "node dist/index.js",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "tap",
    "advertising",
    "cli",
    "campaign",
    "media"
  ],
  "author": "Tap.co",
  "license": "MIT",
  "repository": {
